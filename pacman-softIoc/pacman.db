record (stringout, "$(NAME):SS_$(SS)")
{
    field (DESC, "Status for StateMachine $(SS)")
    field (UDF, "0")
    field (SEVR,"NO_ALARM")
    field (VAL,"INITIAL VALUE")
    field (FLNK, "$(NAME):$(SS)_log15")
}

